{"\$(NAME):", "\$(ASYNPORT)_DICNT_S_CH07_RBV",     "\$(ASYNPORT)_DICNT_S_RBV",     07,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_S_CH08_RBV",     "\$(ASYNPORT)_DICNT_S_RBV",     08,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
