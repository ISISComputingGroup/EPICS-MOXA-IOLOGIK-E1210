# ##############################################################################
# ##############################################################################

# asyn record for the underlying asyn octet port
file "$(ASYN)/db/asynRecord.db" { pattern
{P,R, PORT, ADDR,    IMAX,    OMAX}
{"\$(NAME):", "\$(ASYNPORT)_OctetAsyn", "\$(ASYNPORT)",   0,      80,     80}
}

# ##############################################################################
# DIs in "DI mode" (default)
# ##############################################################################

file "bi_bit.template" { pattern
{P,           R,         PORT,           OFFSET,   ZNAM,   ONAM,  ZSV,       OSV,    SCAN}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH00",     "\$(ASYNPORT)_DI",     00,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH01",     "\$(ASYNPORT)_DI",     01,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH02",     "\$(ASYNPORT)_DI",     02,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH03",     "\$(ASYNPORT)_DI",     03,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH04",     "\$(ASYNPORT)_DI",     04,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH05",     "\$(ASYNPORT)_DI",     05,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH06",     "\$(ASYNPORT)_DI",     06,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH07",     "\$(ASYNPORT)_DI",     07,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH08",     "\$(ASYNPORT)_DI",     08,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH09",     "\$(ASYNPORT)_DI",     09,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH10",     "\$(ASYNPORT)_DI",     0xA,       Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH11",     "\$(ASYNPORT)_DI",     0xB,       Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH12",     "\$(ASYNPORT)_DI",     0xC,       Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH13",     "\$(ASYNPORT)_DI",     0xD,       Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH14",     "\$(ASYNPORT)_DI",     0xE,       Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DI_CH15",     "\$(ASYNPORT)_DI",     0xF,       Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
}

file "statistics.template" { pattern
{P,           R,         PORT,          SCAN}
{"\$(NAME):", "\$(ASYNPORT)_DI_CHx_",     "\$(ASYNPORT)_DI",  "10 second"}
}

file "poll_delay.template" { pattern
{P,           R,              PORT}
{"\$(NAME):", "\$(ASYNPORT)_DI_CHx_PollDelay", "\$(ASYNPORT)_DI"}
}

# #########################################################################################
# Write to single DI in "Counter mode" (must be set in Firmware per a specific DI channel)
# #########################################################################################

file "statistics.template" { pattern
{P,           R,          PORT,           SCAN}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_S_CHx_",     "\$(ASYNPORT)_DICNT_S",  "10 second"}
}

file "bi_bit.template" { pattern
{P,           R,         PORT,              OFFSET,  ZNAM,   ONAM,  ZSV,       OSV,    SCAN}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_S_CH00_RBV",     "\$(ASYNPORT)_DICNT_S_RBV",     00,        Low,    High,  NO_ALARM,  NO_ALARM,  "I/O Intr"}
