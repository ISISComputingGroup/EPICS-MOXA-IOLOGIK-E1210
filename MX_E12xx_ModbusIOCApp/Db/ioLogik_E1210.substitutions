# ###############
# Clear a counter
# ###############

file "bo_bit.template" { pattern
{P,           R,          PORT,          OFFSET,   ZNAM,   ONAM}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH00",     "\$(ASYNPORT)_DICNT_CLR",     00,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH01",     "\$(ASYNPORT)_DICNT_CLR",     01,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH02",     "\$(ASYNPORT)_DICNT_CLR",     02,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH03",     "\$(ASYNPORT)_DICNT_CLR",     03,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH04",     "\$(ASYNPORT)_DICNT_CLR",     04,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH05",     "\$(ASYNPORT)_DICNT_CLR",     05,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH06",     "\$(ASYNPORT)_DICNT_CLR",     06,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH07",     "\$(ASYNPORT)_DICNT_CLR",     07,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH08",     "\$(ASYNPORT)_DICNT_CLR",     08,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH09",     "\$(ASYNPORT)_DICNT_CLR",     09,        Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH10",     "\$(ASYNPORT)_DICNT_CLR",     0xA,       Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH11",     "\$(ASYNPORT)_DICNT_CLR",     0xB,       Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH12",     "\$(ASYNPORT)_DICNT_CLR",     0xC,       Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH13",     "\$(ASYNPORT)_DICNT_CLR",     0xD,       Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH14",     "\$(ASYNPORT)_DICNT_CLR",     0xE,       Low,    High}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CH15",     "\$(ASYNPORT)_DICNT_CLR",     0xF,       Low,    High}
}

file "statistics.template" { pattern
{P,           R,          PORT,           SCAN}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CLR_CHx_",     "\$(ASYNPORT)_DICNT_CLR",  "10 second"}
}

file "longinInt32.template" { pattern
{P,        R,                      PORT,              OFFSET, DATA_TYPE, SCAN}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH00",  "\$(ASYNPORT)_DICNT", 0,      INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH01",  "\$(ASYNPORT)_DICNT", 2,      INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH02",  "\$(ASYNPORT)_DICNT", 4,      INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH03",  "\$(ASYNPORT)_DICNT", 6,      INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH04",  "\$(ASYNPORT)_DICNT", 8,      INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH05",  "\$(ASYNPORT)_DICNT", 0xA,    INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH06",  "\$(ASYNPORT)_DICNT", 0xC,    INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH07",  "\$(ASYNPORT)_DICNT", 0xE,    INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH08",  "\$(ASYNPORT)_DICNT", 11,     INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH09",  "\$(ASYNPORT)_DICNT", 13,     INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH10",  "\$(ASYNPORT)_DICNT", 15,     INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH11",  "\$(ASYNPORT)_DICNT", 17,     INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH12",  "\$(ASYNPORT)_DICNT", 19,     INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH13",  "\$(ASYNPORT)_DICNT", 0x1B,   INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH14",  "\$(ASYNPORT)_DICNT", 0x1D,   INT32_BE,  "I/O Intr"}
{"\$(NAME):", "\$(ASYNPORT)_DICNT_CH15",  "\$(ASYNPORT)_DICNT", 0x1F,   INT32_BE,  "I/O Intr"}
}

